{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "wireit",
    "lint": "wireit",
    "format": "wireit"
  },
  "wireit": {
    "build": {
      "command": "vite build",
      "files": [
        "src",
        "public",
        "index.html",
        "tsconfig.json",
        "tsconfig.node.json",
        "vite.config.ts"
      ],
      "clean": "if-file-deleted",
      "output": [
        "dist/**"
      ],
      "dependencies": [
        "format",
        "lint"
      ]
    },
    "lint": {
      "command": "eslint --ext js,js.cjs,jsx,ts,tsx --color --cache --cache-location .eslintcache . && tsc",
      "files": [
        "*/**.{js,js.cjs,jsx,ts,tsx}",
        ".eslintrc.json",
        "!public",
        "!dist"
      ],
      "output": []
    },
    "format": {
      "command": "prettier '**/*' --write --ignore-unknown --cache",
      "files": [
        "*/**.{js,js.cjs,jsx,ts,tsx,json,md,yml,yaml,html}",
        "!public",
        "!dist"
      ],
      "output": []
    }
  },
  "devDependencies": {
    "@ant-design/cssinjs": "^1.17.2",
    "@generouted/react-router": "^1.16.1",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "antd": "^5.11.1",
    "autoprefixer": "^10.4.16",
    "dayjs": "^1.11.10",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "wireit": "^0.14.1"
  }
}
